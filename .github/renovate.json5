{
	$schema: 'https://docs.renovatebot.com/renovate-schema.json',
	extends: [
		'config:recommended',
	],
	timezone: 'Asia/Tokyo',
	lockFileMaintenance: {
		enabled: true,
		schedule: [
			'before 3:00am',
		],
	},
	automerge: true,
	automergeStrategy: 'rebase',
	nix: {
		enabled: true,
	},
	configMigration: true,
}
